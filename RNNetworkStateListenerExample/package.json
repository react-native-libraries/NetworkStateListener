{
  "name": "RNNetworkStateListenerExample",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "reinstall:js:packages": "rm -rf yarn.lock && rm -rf package-lock.json && rm -rf node_modules && yarn install",
    "link:packages": "yarn react-native link",
    "bundle:ios": "rm -f ./ios/main.jsbundle && react-native bundle --reset-cache --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "reinstall:ios:packages": "yarn reinstall:js:packages && yarn link:packages && cd ios && rm -rf Pods && rm -rf Podfile.lock && pod install && cd .. && yarn bundle:ios"
  },
  "dependencies": {
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-network-state-listener": "../../react-native-network-state-listener"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/runtime": "^7.13.8",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.20.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.65.1",
    "react-test-renderer": "16.13.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
